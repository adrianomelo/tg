    <!-- list of all modules -->

    <!-- Basic API -->
    <property name="api.class" value="${build.dir}/api/${class}"/>
    <property name="api.test.class" value="${build.dir}/api/${class.test}"/>
    <property name="api.jar" value="${build.dir}/${owlapi}/lib/api.jar"/>
    <property name="api.test.jar" value="${build.dir}/${owlapi}/lib/api_test.jar"/>

    <!-- Default Implementation -->
    <property name="impl.class" value="${build.dir}/impl/${class}"/>
    <property name="impl.test.class" value="${build.dir}/impl/${class.test}"/>
    <property name="impl.jar" value="${build.dir}/${owlapi}/lib/impl.jar"/>
    <property name="impl.test.jar" value="${build.dir}/${owlapi}/lib/impl_test.jar"/>

    <!-- RDF Parser -->
    <property name="rdfparser.class" value="${build.dir}/rdfparser/${class}"/>
    <property name="rdfparser.test.class" value="${build.dir}/rdfparser/${class.test}"/>
    <property name="rdfparser.jar" value="${build.dir}/${owlapi}/lib/rdfparser.jar"/>
    <property name="rdfparser.test.jar" value="${build.dir}/${owlapi}/lib/rdfparser_test.jar"/>

    <!-- IO-->
    <property name="io.class" value="${build.dir}/io/${class}"/>
    <property name="io.test.class" value="${build.dir}/io/${class.test}"/>
    <property name="io.jar" value="${build.dir}/${owlapi}/lib/io.jar"/>
    <property name="io.test.jar" value="${build.dir}/${owlapi}/lib/io_test.jar"/>

