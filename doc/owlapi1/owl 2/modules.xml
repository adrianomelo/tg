    <property name="validation.jar" value="${build.dir}/${owlapi}/lib/validation.jar"/>
    <property name="validation.test.jar" value="${build.dir}/${owlapi}/lib/validation_test.jar"/>

    <!-- Inference -->
    <property name="inference.class" value="${build.dir}/inference/${class}"/>
    <property name="inference.test.class" value="${build.dir}/inference/${class.test}"/>
    <property name="inference.jar" value="${build.dir}/${owlapi}/lib/inference.jar"/>
    <property name="inference.test.jar" value="${build.dir}/${owlapi}/lib/inference_test.jar"/>


    <!-- Tests -->
    <property name="owltest.class" value="${build.dir}/owltest/${class}"/>
